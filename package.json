{
  "name": "reliability-prediction-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist/electron/main.js",
  "homepage": "./",
  "scripts": {
    "dev": "vite",
    "server": "uv run uvicorn app.main:app --reload",
    "start": "concurrently \"bun run server\" \"bun run dev\"",
    "dev-electron": "wait-on http://localhost:5173 && electron .",
    "start-electron": "concurrently \"bun run server\" \"bun run dev\" \"bun run dev-electron\"",
    "build": "vue-tsc -b && vite build",
    "build-electron": "bun run build && tsc -p tsconfig.electron.json",
    "electron-build": "bun run build-electron && electron-builder",
    "preview": "vite preview"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "axios": "^1.13.2",
    "chart.js": "^4.5.1",
    "html2pdf.js": "^0.12.1",
    "markdown-it": "^14.1.0",
    "papaparse": "^5.5.3",
    "vue": "^3.5.26",
    "vue-chartjs": "^5.3.3",
    "vue-i18n": "9",
    "vuetify": "^3.11.4"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^25.0.3",
    "@types/papaparse": "^5.5.2",
    "@vitejs/plugin-vue": "^5.2.4",
    "concurrently": "^9.2.1",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "electron-is-dev": "^3.0.1",
    "typescript": "~5.6.3",
    "vite": "^6.4.1",
    "vue-tsc": "^2.2.12",
    "wait-on": "^9.0.3"
  }
}